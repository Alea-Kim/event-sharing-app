<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eventsharing.watudu">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".REGISTER" android:theme="@style/AppTheme.Dark" />
        <activity android:name=".CreateEvent" android:theme="@style/AppTheme.Dark" />
        <activity android:name=".Menu" android:theme="@style/AppTheme.Dark"/>
        <activity android:name=".TAB" android:theme="@style/AppTheme.Dark"/>
        <activity android:name=".EVENTDESC" android:theme="@style/AppTheme.Dark"/>
        <activity android:name=".EVENTS" android:theme="@style/AppTheme.Dark"/>
        <activity android:name=".ActivityLogin" android:theme="@style/AppTheme.Dark"/>
        <activity android:name=".ActivityRegister" android:theme="@style/AppTheme.Dark"/>
        <activity android:name=".TabLayout" android:theme="@style/AppTheme.Dark"/>
        <activity android:name=".MainLogged" android:theme="@style/AppTheme.Dark"/>

        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
             App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>
